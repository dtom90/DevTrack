{
  "name": "produc-dev",
  "version": "0.1.0",
  "private": true,
  "main" : "electron.js",
  "description": "An app for productive developers",
  "author": {
    "name": "David Thomason",
    "url": "https://davidthomason.com/"
  },
  "build": {
    "appId": "produc.dev",
    "mac": {
      "category": "your.app.category.type"
    },
    "files": [
      "package.json",
      "electron.js",
      "dist-electron/**/*"
    ],
    "extends": null
  },
  "scripts": {
    "dev:web": "vue-cli-service serve",
    "dev:electron": "vue-cli-service serve & sleep 5 && NODE_ENV=DEV electron .",
    "build:web": "vue-cli-service build --dest dist-web",
    "build:gh-pages": "vue-cli-service build --mode gh-pages --dest dist-web",
    "build:electron": "vue-cli-service build --mode electron --dest dist-electron",
    "serve:web": "node web.js",
    "serve:gh-pages": "BASE_URL=/Produc-dev/ node web.js",
    "serve:electron": "electron .",
    "prod:web": "yarn run build:web && yarn run serve:web",
    "prod:gh-pages": "yarn run build:gh-pages && yarn run serve:gh-pages",
    "prod:electron": "yarn run build:electron && yarn run serve:electron",
    "pack:electron": "yarn run build:electron && electron-builder",
    "lint": "vue-cli-service lint",
    "lint:fix": "vue-cli-service lint --fix",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e:dev": "testcafe chrome tests/e2e -c 3 --selector-timeout 3000 --app \"yarn run dev\"",
    "test:e2e:prod": "yarn run build:gh-pages && PORT=5000 BASE_URL=/Produc-dev/ testcafe chrome:headless tests/e2e -c 3 --selector-timeout 3000 --app \"yarn run serve:gh-pages\"",
    "test": "yarn run lint && yarn run test:unit && yarn run test:e2e:prod"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.19",
    "@fortawesome/free-solid-svg-icons": "^5.9.0",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "bootstrap": "^4.3.1",
    "chart.js": "^2.8.0",
    "chartjs-plugin-datalabels": "^0.6.0",
    "jquery": "^3.4.1",
    "moment": "^2.24.0",
    "sass": "^1.21.0",
    "sass-loader": "^7.1.0",
    "vue": "^2.6.10",
    "vue-chartjs": "^3.4.2",
    "vuedraggable": "^2.23.0",
    "vuex": "^3.1.1",
    "vuex-persist": "^2.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.9.2",
    "@vue/cli-plugin-eslint": "^3.9.2",
    "@vue/cli-plugin-unit-jest": "^3.9.0",
    "@vue/cli-service": "^3.9.3",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.8.0",
    "electron": "^5.0.6",
    "electron-builder": "^21.2.0",
    "electron-webpack": "^2.7.4",
    "eslint": "^6.1.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.13.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-testcafe": "^0.2.1",
    "eslint-plugin-vue": "^5.2.3",
    "express": "latest",
    "testcafe": "^1.3.3",
    "vue-template-compiler": "^2.6.7"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
